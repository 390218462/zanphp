{
    "name": "zanphp/zan",
    "repositories": {
        "0": {
            "type": "git",
            "url": "https://git.oschina.net/zan-group/nova.git"
        },
        "1": {
            "type": "git",
            "url": "https://git.oschina.net/zan-group/nova-generic.git"
        },
        "packagist": {
            "type": "composer",
            "url": "https://packagist.phpcomposer.com"
        }
    },
    "require": {
        "nova-service/generic": "dev-master",
        "zanphp/nova": "dev-master",
        "packaged/thrift": "0.9.2.1",
        "symfony/console": "3.1",
        "psr/log": "1.0.0"
    },
    "autoload": {
        "psr-4": {
            "Zan\\Framework\\": "src/"
        },
        "classmap": [],
        "files": [
            "src/Foundation/Coroutine/Command/Task.php",
            "src/Utilities/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Zan\\Framework\\Test\\": "test/"
        },
        "classmap": [
            "test/TestCase.php"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "secure-http": false
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
